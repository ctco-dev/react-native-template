{
  "compilerOptions": {
    "allowJs": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "es6"
    ],
    "outDir": "compiled",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules",
    "compiled",
    "storybook"
  ]
}
